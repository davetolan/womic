{
  "name": "womic-workspace",
  "private": true,
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "scripts": {
    "dev": "pnpm --filter next-comic-platform dev",
    "migrate": "pnpm --filter next-comic-platform payload migrate",
    "build": "pnpm --filter next-comic-platform build",
    "build:vercel": "pnpm run migrate && pnpm run build",
    "build:vercelfresh": "pnpm payload migrate:fresh && pnpm run build",
    "start": "pnpm --filter next-comic-platform start",
    "lint": "pnpm --filter next-comic-platform lint"
  },
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": "20.x",
    "pnpm": "10.x"
  },
  "dependencies": {
    "cloudinary": "^2.9.0"
  }
}
